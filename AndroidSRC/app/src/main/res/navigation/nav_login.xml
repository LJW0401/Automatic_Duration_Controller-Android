<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_login"
    app:startDestination="@id/loginFragment_loginNav">

    <fragment
        android:id="@+id/loginFragment_loginNav"
        android:name="com.example.AutomaticDurationControl.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_mainFragment2"
            app:destination="@id/mainFragment_loginNav" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment_loginNav"
        android:name="com.example.AutomaticDurationControl.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" />
</navigation>